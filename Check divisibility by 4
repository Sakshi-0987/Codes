import java.util.*;

class Main {

    public static void main(String[] args) {

    Scanner scn = new Scanner (System.in);
    //to check divisibilty
    int x = scn.nextInt();
    if (x%4 == 0) {
        System.out.print ("Divisible by 4");
    }
    else {
        System.out.print ("Not divisible by 4");
    }
    
   }
   }
    
